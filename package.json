{
	"name": "sm-react-common-alert",
	"version": "1.0.2",
	"description": "Common frontend components for SM",
	"main": "dist/index.js",
	"scripts": {
		"start": "webpack-dev-server --hot --inline --mode development",
		"prepublishOnly": "npm run release",
		"release": "npm run build:dev && npm run build:prod",
		"build:prod": "cross-env NODE_ENV=production rollup -c",
		"build:dev": "cross-env NODE_ENV=development rollup -c",
		"publish": "npm publish",
		"test": "jest",
		"test:coverage": "jest --coverage --watch",
		"test:watch": "npm run test -- --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/magarampage/sm-front-common.git"
	},
	"keywords": [
		"react",
		"components"
	],
	"author": "magarampage",
	"license": "BSD-3-Clause",
	"bugs": {
		"url": "https://github.com/magarampage/sm-front-common/issues"
	},
	"homepage": "https://github.com/magarampage/sm-front-common#readme",
	"peerDependencies": {
		"prop-types": "^15.6.2",
		"react": "^16.0.0",
		"react-alert": "2.4.0",
		"react-dom": "^16.0.0"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.5",
		"babel-plugin-external-helpers": "^6.22.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"cross-env": "^5.2.0",
		"css-loader": "^1.0.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.4.1",
		"prop-types": "^15.6.2",
		"react": "^16.3.1",
		"react-alert": "2.4.0",
		"react-dom": "^16.3.1",
		"rollup": "^0.58.2",
		"rollup-plugin-babel": "^3.0.4",
		"rollup-plugin-commonjs": "^9.1.0",
		"rollup-plugin-node-resolve": "^3.3.0",
		"rollup-plugin-replace": "^2.0.0",
		"webpack": "^4.15.1",
		"webpack-cli": "^3.0.8",
		"webpack-dev-server": "^3.1.4"
	},
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"src/**/*.js"
		],
		"transform": {
			".*": "./node_modules/jest-css-modules"
		},
		"moduleNameMapper": {
			"\\.(css|less)$": "identity-obj-proxy"
		}
	}
}
