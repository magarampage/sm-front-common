	"author": "magarampage",
	"license": "BSD-3-Clause",
	"bugs": {
		"url": "https://github.com/magarampage/sm-front-common/issues"
	},
	"homepage": "https://github.com/magarampage/sm-front-common#readme",
	"peerDependencies": {
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"react-alert": "2.4.0"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.3",
		"babel-jest": "^22.4.3",
		"babel-loader": "^7.1.5",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.22.0",
