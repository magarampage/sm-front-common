		"babel-preset-react-hmre": "^1.1.1",
		"babel-preset-stage-0": "^6.5.0",
		"cross-env": "^5.1.6",
		"css-loader": "^0.28.10",
		"eslint": "4.9.0",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-plugin-import": "2.7.0",
		"eslint-plugin-jsx-a11y": "6.0.2",
